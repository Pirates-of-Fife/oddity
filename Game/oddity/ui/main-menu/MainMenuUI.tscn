[sub_resource type="GDScript" id="GDScript_lnw6i"]
resource_name = "creature_sensitivity"
script/source = "extends HSlider

func _ready() -> void:
	value = Globals.creature_controller_sensitivity

func _on_value_changed(value: float) -> void:
	Globals.creature_controller_sensitivity = value
	tooltip_text = \"Sensitivity: \" + str(value)


func _on_drag_ended(value_changed: bool) -> void:
	Globals.save_settings()
"

